{
  "expo": {
    "name": "Waddle",
    "slug": "waddle",
    "scheme": "waddle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anas.waddle.final",
      "infoPlist": {
        "NSReminderUsageDescription": "Waddle needs this to remind you of your daily habits.",
        "NSLocalNetworkUsageDescription": "Waddle needs this to connect to the development server.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.785903617233-91vrbkqufgiq0j3up2veketjtl2dise2"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anas.waddle.xfinal"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization",
      "expo-notifications",
      "@react-native-google-signin/google-signin"
    ],
    "extra": {
      "eas": {
        "projectId": "f1cc043c-2a2e-44fb-809d-b12716e65229"
      }
    },
    "owner": "codepulse2000"
  }
}